{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Place list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Name</th>
                <th>Address</th>
                <th>City</th>
                <th>Zip</th>
                <th>Secretcode</th>
                <th>Ormlink</th>
                <th>Gps_lat</th>
                <th>Gps_long</th>
                <th>Gps_alt</th>
                <th>Compas_x</th>
                <th>Compas_y</th>
                <th>Compas_z</th>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('ubu_place_show', { 'id': entity.id }) }}">{{ entity.name }}</a></td>
                <td>{{ entity.address }}</td>
                <td>{{ entity.city }}</td>
                <td>{{ entity.zip }}</td>
                <td>{{ entity.secretCode }}</td>
                <td>{{ entity.ormLink }}</td>
                <td>{{ entity.gpslat }}</td>
                <td>{{ entity.gpslong }}</td>
                <td>{{ entity.gpsalt }}</td>
                <td>{{ entity.compasx }}</td>
                <td>{{ entity.compasy }}</td>
                <td>{{ entity.compasz }}</td>
                <td>{{ entity.id }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('ubu_place_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('ubu_place_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('ubu_place_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
